{
  "compilerOptions": {
    "types": ["bun-types"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "moduleDetection": "force",
    "strict": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "esModuleInterop": true
  },
  "include": ["src"]
}
